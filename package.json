{
  "name": "@indutny/sneaky-equals",
  "version": "3.0.1",
  "description": "Sneaky equality check between objects using proxies",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "c8 ava test/*.ts",
    "lint": "eslint --cache .",
    "prettier": "prettier --cache --write ."
  },
  "keywords": [
    "proxy",
    "equality"
  ],
  "author": "Fedor Indutny <238531+indutny@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indutny/sneaky-equals.git"
  },
  "bugs": {
    "url": "https://github.com/indutny/sneaky-equals/issues"
  },
  "homepage": "https://github.com/indutny/sneaky-equals#readme",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "ava": "^5.1.0",
    "c8": "^7.12.0",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
