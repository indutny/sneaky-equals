{
  "name": "@indutny/sneequals",
  "version": "1.3.5",
  "description": "Sneaky equality check between objects using proxies",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "c8 ava test/*.ts",
    "lint": "eslint --cache .",
    "format": "prettier --cache --write .",
    "check:format": "prettier --cache --check .",
    "prepublish": "rm -rf dist && npm run build",
    "bench": "bencher dist/benchmarks/*.js"
  },
  "keywords": [
    "proxy",
    "equality"
  ],
  "author": "Fedor Indutny <238531+indutny@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indutny/sneequals.git"
  },
  "bugs": {
    "url": "https://github.com/indutny/sneequals/issues"
  },
  "homepage": "https://github.com/indutny/sneequals#readme",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@indutny/bencher": "^1.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "ava": "^5.1.0",
    "c8": "^7.12.0",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
